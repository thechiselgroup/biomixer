<module rename-to='org.thechiselgroup.biomixer.BioMixerWorkbench'>

  <inherits name='org.thechiselgroup.choosel.workbench.Workbench'/>
  <inherits name='org.thechiselgroup.choosel.visualization_component.chart.Chart'/>
  <inherits name='org.thechiselgroup.choosel.visualization_component.graph.Graph'/>
  <inherits name='org.thechiselgroup.choosel.visualization_component.text.Text'/>
  <inherits name='org.thechiselgroup.choosel.visualization_component.timeline.Timeline'/>
  
  <!-- client-side logging configuration -->
  <!-- TODO deactivate for deployment --> 
  <set-property name="gwt.logging.enabled" value="TRUE"/> 
  <set-property name="gwt.logging.logLevel" value="INFO"/>
  <set-property name="gwt.logging.popupHandler" value="DISABLED"/>
  <set-property name="gwt.logging.systemHandler" value="DISABLED"/>
  <set-property name="gwt.logging.consoleHandler" value="ENABLED"/>
  <set-property name="gwt.logging.firebugHandler" value="ENABLED"/>
  <set-property name="gwt.logging.simpleRemoteHandler" value="DISABLED"/>
  <set-property name="gwt.logging.developmentModeHandler" value="ENABLED"/>  
  
  <entry-point class='org.thechiselgroup.biomixer.client.BioMixerEntryPoint'/>
  
</module>